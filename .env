# Claude Orchestrator Environment Variables
# Copy this file to .env and fill in your values

# Git Bash Path (Windows)
# Example: C:\Program Files\Git\bin\bash.exe
CLAUDE_CODE_GIT_BASH_PATH=C:\opt\Git.Git\usr\bin\bash.exe

# Claude Authentication (DEPRECATED - Use WSL Claude CLI login instead)
# WSL環境でClaude CLIが自動インストールされます
# 使用前にWSL内で `claude` コマンドを実行してログインしてください
# CLAUDE_API_TOKEN=your_token_here  # Not used

# Orchestrator Configuration
MAX_WORKERS=8
MAX_DEPTH=3
TIMEOUT_SECONDS=300

# Logging Level
LOG_LEVEL=INFO
